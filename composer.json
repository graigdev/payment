{
    "name": "graigdev/payment",
    "description": "A Laravel package that generates payment codes for subscriptions with wallet management",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "GraigDev",
            "email": "graigdev@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0",
        "illuminate/database": "^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0|^7.0|^8.0",
        "phpunit/phpunit": "^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "GraigDev\\Payment\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GraigDev\\Payment\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "GraigDev\\Payment\\PaymentServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
} 